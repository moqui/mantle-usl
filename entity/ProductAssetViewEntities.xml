    <view-entity entity-name="AssetAndDetail" package="mantle.product.asset">
        <member-entity entity-alias="AST" entity-name="mantle.product.asset.Asset"/>
        <member-entity entity-alias="ASD" entity-name="mantle.product.asset.AssetDetail" join-from-alias="AST">
            <key-map field-name="assetId"/></member-entity>
        <alias-all entity-alias="AST"/>
        <alias-all entity-alias="ASD"><exclude field="productId"/></alias-all>
    </view-entity>
    <view-entity entity-name="AssetDetailSummary" package="mantle.product.asset">
        <member-entity entity-alias="ASD" entity-name="mantle.product.asset.AssetDetail"/>
        <alias entity-alias="ASD" name="assetId"/>
        <alias entity-alias="ASD" name="availableToPromiseTotal" field="availableToPromiseDiff" function="sum"/>
        <alias entity-alias="ASD" name="quantityOnHandTotal" field="quantityOnHandDiff" function="sum"/>
    </view-entity>
    <view-entity entity-name="AssetValuationSummary" package="mantle.product.asset">
        <member-entity entity-alias="AST" entity-name="mantle.product.asset.Asset"/>
        <member-entity entity-alias="ASD" entity-name="mantle.product.asset.AssetDetail" join-from-alias="AST">
            <key-map field-name="assetId"/></member-entity>
        <member-entity entity-alias="PRD" entity-name="mantle.product.Product" join-from-alias="AST">
            <key-map field-name="productId"/></member-entity>
        <member-entity entity-alias="ALOT" entity-name="mantle.product.asset.Lot" join-from-alias="AST" join-optional="true">
            <key-map field-name="lotId"/></member-entity>
        <alias-all entity-alias="AST"/>
        <alias-all entity-alias="ASD"><exclude field="productId"/><exclude field="availableToPromiseDiff"/>
            <exclude field="quantityOnHandDiff"/></alias-all>
        <alias entity-alias="ASD" name="availableToPromiseDiff" function="sum"/>
        <alias entity-alias="ASD" name="quantityOnHandDiff" function="sum"/>
        <alias entity-alias="PRD" name="pseudoId"/>
        <alias entity-alias="PRD" name="productName"/>
        <alias entity-alias="ALOT" name="mfgPartyId"/>
        <alias entity-alias="ALOT" name="lotNumber"/>
        <alias name="totalCost" function="sum"><complex-alias operator="*">
            <complex-alias-field entity-alias="ASD" field="quantityOnHandDiff"/>
            <complex-alias-field entity-alias="AST" field="acquireCost"/>
        </complex-alias></alias>
        <alias name="minAcquireCost" entity-alias="AST" field="acquireCost" function="min"/>
        <alias name="maxAcquireCost" entity-alias="AST" field="acquireCost" function="max"/>
    </view-entity>
    <view-entity entity-name="AssetOnHandSummary" package="mantle.product.asset">
        <member-entity entity-alias="AST" entity-name="mantle.product.asset.Asset"/>
        <member-entity entity-alias="ASD" entity-name="mantle.product.asset.AssetDetail" join-from-alias="AST">
            <key-map field-name="assetId"/></member-entity>
        <member-entity entity-alias="PRD" entity-name="mantle.product.Product" join-from-alias="AST">
            <key-map field-name="productId"/></member-entity>
        <alias-all entity-alias="AST"/>
        <alias-all entity-alias="ASD"><exclude field="productId"/><exclude field="availableToPromiseDiff"/>
            <exclude field="quantityOnHandDiff"/></alias-all>
        <alias entity-alias="PRD" name="pseudoId"/>
        <alias entity-alias="PRD" name="productName"/>
        <alias name="startingOnHand" function="sum">
            <!-- use DB time zone instead of default (current user) -->
            <case><when expression="ASD.EFFECTIVE_DATE &lt; '${ec.l10n.format(fromDate, 'yyyy-MM-dd HH:mm:ss', null, ec.entity.databaseTimeZone)}'">
                <complex-alias expression="ASD.QUANTITY_ON_HAND_DIFF"/></when><else><complex-alias expression="0"/></else></case>
        </alias>
        <alias name="onHandChange" function="sum">
            <case><when expression="ASD.EFFECTIVE_DATE BETWEEN '${ec.l10n.format(fromDate, 'yyyy-MM-dd HH:mm:ss', null, ec.entity.databaseTimeZone)}' AND '${ec.l10n.format(thruDate, 'yyyy-MM-dd HH:mm:ss', null, ec.entity.databaseTimeZone)}'">
                <complex-alias expression="ASD.QUANTITY_ON_HAND_DIFF"/></when><else><complex-alias expression="0"/></else></case>
        </alias>
    </view-entity>
